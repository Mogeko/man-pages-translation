#: raw/manpages-dev/man2/socket.2:329 raw/manpages-dev/man2/socket.2:332
#, no-wrap
msgid "B<EINVAL>"
msgstr ""

#. type: Plain text
#: raw/manpages-dev/man2/socket.2:332
msgid "Unknown protocol, or protocol family not available."
msgstr ""

