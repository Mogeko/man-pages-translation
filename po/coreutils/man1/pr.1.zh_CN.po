#, no-wrap
msgid "March 2020"
msgstr "2020年三月"

#. type: TH
#: raw/coreutils/man1/pr.1:2
#, no-wrap
msgid "GNU coreutils 8.32"
msgstr "GNU coreutils 8.32"

#. type: TH
#: raw/coreutils/man1/pr.1:2
#, no-wrap
